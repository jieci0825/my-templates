{
    "name": "vue-admin-ts",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently -n mock,web -c blue,green --kill-others-on-fail \"pnpm run dev:mock\" \"wait-on tcp:localhost:3100 && pnpm run dev:web\"",
        "dev:mock": "pnpm -C mock-server dev",
        "dev:web": "vite dev --mode development",
        "build": "vue-tsc -b && vite build --mode production",
        "build:dev": "vue-tsc -b && vite build --mode development",
        "preview": "vite preview"
    },
    "dependencies": {
        "@iconify/json": "^2.2.435",
        "@vueuse/core": "^14.2.0",
        "@vueuse/integrations": "^14.2.0",
        "axios": "^1.13.4",
        "element-plus": "^2.13.2",
        "nprogress": "^0.2.0",
        "pinia": "^2.3.1",
        "vue": "^3.5.24",
        "vue-router": "^5.0.2"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "@types/nprogress": "^0.2.3",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.8.1",
        "concurrently": "^9.1.2",
        "prettier": "^3.8.1",
        "sass": "^1.97.3",
        "typescript": "~5.9.3",
        "unplugin-auto-import": "^21.0.0",
        "unplugin-icons": "^23.0.1",
        "unplugin-vue-components": "^31.0.0",
        "vite": "^7.2.4",
        "vue-tsc": "^3.1.4",
        "wait-on": "^9.0.4"
    }
}
